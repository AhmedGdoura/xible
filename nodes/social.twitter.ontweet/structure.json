{
  "name": "social.twitter.ontweet",
  "type": "event",
  "description": "Triggered whenever a tweet comes in on the given search tags.",
  "inputs": {
    "twitter": {
  		"type": "social.twitter"
  	}
  },
  "outputs": {
    "trigger": {
  		"type": "trigger"
  	},
    "tweet": {
  		"type": "document"
  	},
    "text": {
  		"type": "string"
  	},
    "username": {
  		"type": "string"
  	}
  }
}
